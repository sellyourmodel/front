{% if products|length > 0 %}
    <ul class="product-list col-4">
        {% for e in products %}
            {% include 'AppBundle:Catalog:_product_in_category.html.twig' with {'e':e} %}
        {% endfor %}
    </ul>
{% else %}
    <div class="product_list--empty">{% trans %}category_not_have_models{% endtrans %}</div>
{% endif %}