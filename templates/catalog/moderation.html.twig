{% extends 'cabinet/_cabinet_base.html.twig' %}

{% block cabinetTitle %}{% trans %}moderation_header{% endtrans %}{% endblock %}

{% block wrapper_class %}catalog{% endblock %}

{% block cabinetContent %}

    {% if products|length > 0 %}
        <ul class="product-list">
            {% for e in products %}
                {% include 'catalog/_product_in_category.html.twig' with {'e':e} %}
            {% endfor %}
        </ul>
    {% else %}
        <div class="product_list--empty">{% trans %}moderation_not_have_models{% endtrans %}</div>
    {% endif %}
{% endblock %}
