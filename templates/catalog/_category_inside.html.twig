{% if products|length > 0 %}
    <ul class="product-list">
        {% for e in products %}
            {% include 'catalog/_product_in_category.html.twig' with {'e':e} %}
        {% endfor %}
    </ul>
    {{ knp_pagination_render(products) }}
{% else %}
    <div class="product_list--empty">{% trans %}category_not_have_models{% endtrans %}</div>
{% endif %}